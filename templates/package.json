    "install_button": %(install_button)s,
    "deploy": {
      "folders": [
        "icons"
      ],
      "files": [
        "manifest.appcache"
      ]
    },
    "environment": {
      "libraries": [
        "jquery",
        "underscore",
        "backbone",
        "handlebars",
        "globalize",
        "helpers",
