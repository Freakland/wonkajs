{
  "name": "wonkaweb",
  "description": "Write a little bit about this project",
  "version": "0.0.1",
  "author": "",
  "homepage": "",
  "_id": "wonkaweb@0.0.1",
  "settings": {
    "storage_engine": "localStorage",
    "i18n": {
      "languages": [
        "en",
        "es"
      ],
      "files": [
        "js",
        "html",
        "hbs"
      ],
      "blacklist": [
        ".git",
        "images",
        "icons",
        "stylesheets",
        "libraries"
      ]
    },
    "deploy": {
      "folders": [
        "icons",
        "fonts",
        "images",
        "languages"
      ]
    },
    "environment": {
      "core": [
        "app",
        "contrib/modernizr"
      ],
      "libraries": [
        "jquery",
        "bootstrap",
        "handlebars",
        "underscore",
        "backbone",
        "globalize",
        "console",
        "helpers"
      ],
      "main_files": [
        "listeners",
        "alerts",
        "main"
      ],
      "applications": [
        "root",
        "demos",
        "demos/todos"
      ]
    }
  }
}